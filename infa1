Задача3
#include <iostream>
#include <math.h>
using namespace std;

int main()
{
    float a, b, c;
    cin >> a;
    cin >> b;
    cin >> c;
    float dic;
    dic = pow(b, 2) - 4ac;

    if (dic < 0)
    {
        cout << "Resheniya net" << endl;
    }
    else if (dic == 0)
    {
        cout << -b/(2a);
    }
    else
    {
        cout << (-b-sqrt(dic))/(2a) << endl;
        cout << (-b+sqrt(dic))/(2*a) << endl;
    }
}

